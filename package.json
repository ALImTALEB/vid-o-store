    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.4",
    "@types/swagger-ui-react": "4.1.1",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.5.1",
    "babel-loader": "8.2.4",
    "css-mediaquery": "0.1.2",
    "eslint": "8.11.0",
    "eslint-config-react-app": "7.0.0",
